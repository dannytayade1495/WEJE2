<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.bean.StudentBean"
		name="student1">
		<property name="id">
			<value>1</value>
		</property>
		<property name="name">
			<value>Arjun</value>
		</property>
		<property name="email">
			<value>arjun@gmail.com</value>
		</property>
		<property name="contact">
			<value>9876543210</value>
		</property>
	</bean>

	<bean 
	class="com.jspiders.springcorexml.bean.StudentBean"
		name="student2">
		<property name="id" value="2" />
		<property name="name" value="Karan" />
		<property name="email" value="karan@gmail.com" />
		<property name="contact" value="8976542310" />
	</bean>

</beans>